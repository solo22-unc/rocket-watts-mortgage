{
	"manifest_version": 3,
	"name": "Rocket Watts Mortgage",
	"version": "0.0.2",
	"description": "Adds 'Presented by Rocket Mortgage' to MSU basketball terms",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"action": {
		"default_popup": "popup.html"
	  },
	"icons": {
		"16": "16x16.png",
		"32": "32x32.png",
		"48": "48x48.png",
		"128": "128x128.png"
	}
}

